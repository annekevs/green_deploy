        _Common_wrapper_js__WEBPACK_IMPORTED_MODULE_0___default()(navbarFormSelector).removeClass('open') // Close control
        .find('input[type="text"]').blur() // remove focus
        // .val('') // Empty input
        ;
      }
    };
  }
